<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
     <!--强制指定MyBatis使用log4j作为日志日志框架，若不指定那么当部署到如Tomcat等应用容器时，会被容器设置为使用common-logging来记录日志-->
    <settings>
        <setting name="logImpl" value="LOG4J"/>
    </settings>
    
    <!-- 类全名 转 别名，已在spring中配置
    <typeAliases>
    	<typeAlias alias="User" type="com.baobaotao.domain.User" />
    	<typeAlias alias="LoginLog" type="com.baobaotao.domain.LoginLog" />
    </typeAliases> -->
</configuration>